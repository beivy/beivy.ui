{
    "name": "@beivy/beivy.ui",
    "version": "0.0.1-27",
    "license": "MIT",
    "main": "./es/index.js",
    "jsnext:main": "./es/index.js",
    "types": "types/index.d.ts",
    "repoistory": "beivy/beivy.ui",
    "files": [
        "es",
        "lib",
        "src",
        "types"
    ],
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c -w",
        "build-storybook": "build-storybook",
        "css": "npx tailwindcss -i ./src/global.css -o ./tailwind.css",
        "css:watch": "DEBUG=true npx tailwindcss -i ./src/global.css -o ./tailwind.css --watch",
        "icons": "npx @svgr/cli -d src/icon/svg assets/svgs --typescript",
        "start": "npm run dev",
        "storybook": "start-storybook -p 6006",
        "test": "jest",
        "clean": "rimraf dist",
        "publish:prerelease": "yarn publish --prerelease --no-git-tag-version --no-commit-hooks --access public",
        "publish:patch": "yarn publish --patch ",
        "prerelease": "npm-run-all clean build publish:prerelease"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.7",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.8",
        "@types/node": "^18.7.13",
        "papaparse": "^5.3.2",
        "ramda": "^0.28.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-dropzone": "^14.2.3",
        "react-hook-form": "^7.41.5",
        "tailwindcss": "^3.1.8",
        "tailwindcss-classnames": "^3.0.6",
        "typescript": "^4.8.2"
    },
    "devDependencies": {
        "@babel/core": "^7.19.6",
        "@storybook/addon-actions": "^6.5.14",
        "@storybook/addon-console": "^1.2.3",
        "@storybook/addon-essentials": "^6.5.13",
        "@storybook/addon-interactions": "^6.5.13",
        "@storybook/addon-links": "^6.5.13",
        "@storybook/builder-webpack4": "^6.5.13",
        "@storybook/manager-webpack4": "^6.5.13",
        "@storybook/react": "^6.5.13",
        "@storybook/testing-library": "^0.0.13",
        "@svgr/cli": "^6.5.1",
        "@swc/core": "^1.2.244",
        "@swc/jest": "^0.2.22",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.2.0",
        "@types/papaparse": "^5.3.7",
        "@types/ramda": "^0.28.20",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "babel-loader": "^8.2.5",
        "install-peers": "^1.0.4",
        "jest": "^29.2.2",
        "npm-run-all": "^4.1.5",
        "react-resize-panel": "^0.3.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.78.1",
        "rollup-plugin-typescript2": "^0.33.0",
        "snapshot-diff": "^0.10.0",
        "ts-jest": "^29.0.3"
